{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
      "standard",
      "eslint:recommended",
      "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "max-len": ["error", {"code": 120, "ignoreUrls": true}],
        "no-tabs": ["error", {"allowIndentationTabs": true}],
        "indent": ["error", 2]
    }
}
